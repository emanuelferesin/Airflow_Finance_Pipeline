# --- CREDENCIALES DE LAS 3 APIS ---
# API 1: Precios de Acciones (Temporal)
API_KEY_FINANCE=TU_KEY_DE_LA_API_DE_PRECIOS

# API 2: Metadatos de Seguridad (Para SCD2)
API_KEY_METADATA=TU_KEY_DE_LA_API_DE_METADATOS

# API 3: Tasa de Cambio Dólar (Temporal)
API_KEY_DOLAR=TU_KEY_DE_LA_API_DEL_DOLAR

# --- CREDENCIALES DE AIRFLOW/REDSHIFT ---
# El evaluador debe usarlas para configurar la conexión 'redshift_conn' en Airflow UI.
AIRFLOW_CONN_REDSHIFT_HOST=
AIRFLOW_CONN_REDSHIFT_LOGIN=
AIRFLOW_CONN_REDSHIFT_PASSWORD=
AIRFLOW_CONN_REDSHIFT_SCHEMA=